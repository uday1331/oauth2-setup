[   
  {
    "id": "connect-to-serice",
    "upstream": {
      "url": "https://postman-echo.com/",
      "preserve_host":true
    },
    "match": {
      "url": "http://<127.0.0.1|localhost>:4455/<.*>",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [{ "handler": "noop" }],
    "authorizer": { "handler": "allow" },
    "mutators": [{ "handler": "noop" }],
    "errors": [
        {
          "handler": "json"
        }
      ]
  }
]
